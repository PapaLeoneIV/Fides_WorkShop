generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Order {
  id       String  @id @default(uuid())
  road     String
  dirt     String
}

model Bikes {
  id       String  @id @default(uuid())
  road     Int
  dirt     Int

  @@map("Bikes") // Maps the model to the "Bikes" table in your database
}